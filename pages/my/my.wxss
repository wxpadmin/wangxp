page {
  background-color: #f6f6f5
}
.wrap{
  width: 100%
}

/* 用户信息 */
.user{
  width: 100%;
  height: 447rpx;
  box-sizing: border-box;
  padding: 56rpx 46rpx 0;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  display: flex;
  justify-content: space-around;
}
.user_left{
  display: flex;
}


.user_left_avatar{
  width: 170rpx;
  height: 170rpx;
  margin-right: 30rpx
}
.user_left_avatar>image{
  width: 100%;
  height: 100%;
  border-radius: 50%
}
.user_left_userinfo{
  display: flex;
  flex-direction: column;
}
.user_left_userinfo>text{
  color: #fff
}
.user_left_userinfo>text:nth-of-type(2){
  margin: 16rpx 0 20rpx 0
}
.user_left_userinfo>image{
  height: 37rpx;
  width: 125rpx
}

.user_right>image{
  width: 180rpx;
  height: 138rpx;
  margin-top: 18rpx
}

/*  导航 */
.navigation{
  width: 690rpx;
  height: 204rpx;
  box-shadow: 0 0 30rpx rgba(175,27,8,0.1);
  margin:  -130rpx auto 0;
  background: #fff;
  border-radius: 10rpx
}